{
  "name": "@modern-js/inspector-webpack-plugin",
  "version": "1.0.0-alpha.3",
  "description": "Inspector for webpack",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ttsc -w",
    "build": "rm -rf dist tsconfig.tsbuildinfo && ttsc",
    "test": "pnpm run test-unit && pnpm run test-e2e",
    "test-unit": "vitest run",
    "test-e2e": "vitest run -c vitest.config.e2e.ts"
  },
  "keywords": [
    "webpack"
  ],
  "author": "yangxingyuan",
  "license": "MIT",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.4",
    "@types/koa": "^2.13.4",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^17.0.31",
    "@types/resolve": "^1.20.2",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "playwright-chromium": "^1.25.0",
    "tsconfig-paths": "^4.0.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3",
    "typescript-transform-paths": "^3.3.1",
    "vitest": "^0.22.1",
    "webpack": "^5.73.0"
  },
  "files": [
    "dist",
    "client"
  ],
  "dependencies": {
    "@koa/cors": "^3.3.0",
    "koa": "^2.13.4",
    "koa-router": "^11.0.1",
    "koa-static": "^5.0.0",
    "picocolors": "^1.0.0"
  }
}
